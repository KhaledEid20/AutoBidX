{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "auctions" : {
        "ClusterId": "auctions",
        "Match": {
          "Path": "/auction/{**catch-all}"
        },
        "Transforms":[
          {
            "PathPattern" : "api/auction/{**catch-all}"
          }
        ]
      },
      "search" : {
        "ClusterId": "search",
        "Match": {
          "Path": "/search/{**catch-all}",
          "Method" : "GET"
        },
        "Transforms":[
          {
            "PathPattern" : "api/search/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "auctions": {
        "Destinations": {
          "auctionApi": {
            "Address": "http://localhost:7001"
          }
        }
      },
      "search": {
        "Destinations": {
          "sarchApi": {
            "Address": "http://localhost:7002"
          }
        }
      }
    }
  }
}
